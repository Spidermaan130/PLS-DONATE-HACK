local function NewScroll(pos, size)
    local s = Instance.new("ScrollingFrame", Main)
    s.Size = size; s.Position = pos; s.BackgroundTransparency = 1; s.ScrollBarThickness = 3; s.CanvasSize = UDim2.new(0,0,0,0); s.AutomaticCanvasSize = "Y"
