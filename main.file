local function Trans(key, v1, v2)
    local set = Langs[UserLocale] or Langs["en-us"]
    return (v1 and v2) and string.format(set[key], tostring(v1), tostring(v2)) or (v1 and string.format(set[key], tostring(v1)) or set[key])
end

