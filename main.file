-- [[ âš™ï¸ COMMAND LOGIC ]] --
local Target = nil
local CurrentPass = {id = nil, cost = 0}

local function GetStats(p)
    local s = p:FindFirstChild("leaderstats")
    return (s and s:FindFirstChild("Donated") and s.Donated.Value or 0) + (s and s:FindFirstChild("Raised") and s.Raised.Value or 0)
end

local function LoadData()
    for _, p in pairs(game.Players:GetPlayers()) do
        if p ~= LP then
            local b = Instance.new("TextButton", PScroll); b.Size = UDim2.new(1, -10, 0, 40); b.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
            b.Text = p.DisplayName .. " [$" .. GetStats(p) .. "]"; b.TextColor3 = Color3.new(1, 1, 1); b.Font = "GothamBold"; Instance.new("UICorner", b)
            b.MouseButton1Click:Connect(function() Target = p; Log.Text = ">> LOCKED ON: " .. p.Name end)
        end
    end
    -- Auto-Fetch Your Gamepasses
    pcall(function()
        local url = "https://games.roproxy.com/v2/users/"..LP.UserId.."/games?accessFilter=Public&limit=10"
        local games = HttpService:JSONDecode(game:HttpGet(url)).data
        for _, g in pairs(games) do
            local ps = HttpService:JSONDecode(game:HttpGet("https://games.roproxy.com/v1/games/"..g.id.."/game-passes")).data
            for _, pass in pairs(ps) do
                local b = Instance.new("TextButton", RScroll); b.Size = UDim2.new(1, -10, 0, 40); b.BackgroundColor3 = Color3.fromRGB(0, 70, 0)
                b.Text = pass.price .. " R$"; b.TextColor3 = Color3.new(1, 1, 1); b.Font = "GothamBlack"; Instance.new("UICorner", b)
                b.MouseButton1Click:Connect(function() CurrentPass = {id = pass.id, cost = pass.price} end)
            end
        end
    end)
end

Btn.MouseButton1Click:Connect(function()
    if not Target or not CurrentPass.id then return end
    local wealth = GetStats(Target)
    
    if wealth >= CurrentPass.cost then
        Log.TextColor3 = Color3.new(0, 1, 0)
        Log.Text = ">> " .. Translate("success", Target.Name, CurrentPass.cost, wealth)
        
        local root = Target.Character and Target.Character:FindFirstChild("HumanoidRootPart")
        if root then
            local old = root.CFrame
            root.CFrame = LP.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -5)
            MarketService:PromptGamePassPurchase(Target, CurrentPass.id)
            task.wait(5)
            root.CFrame = old
        end
    else
        Log.TextColor3 = Color3.new(1, 0, 0)
        Log.Text = ">> " .. Translate("error", Target.Name, CurrentPass.cost, wealth)
    end
end)

LoadData()
